{
  "name": "eth-hooks-documentation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "cross-env TYPEDOC_WATCH=true docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --config ./.eslintrc --ignore-path ./.eslintignore ./src/**/*.ts* ",
    "format": "prettier -w ./src"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.18",
    "@docusaurus/preset-classic": "2.0.0-beta.18",
    "@docusaurus/theme-common": "^2.0.0-beta.18",
    "@easyops-cn/docusaurus-search-local": "^0.23.2",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.*",
    "react-dom": "^17.*"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.18",
    "@saucelabs/theme-github-codeblock": "^0.1.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "daisyui": "^2.14.3",
    "docusaurus-plugin-typedoc": "^0.17.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-developit": "^1.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "mdx-mermaid": "^1.2.2",
    "mermaid": "^8.11.5",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prettier-eslint": "^14.0.2",
    "prettier-markdown": "^0.1.8",
    "tailwindcss": "^3.0.24",
    "typedoc": "^0.22.15",
    "typedoc-plugin-merge-modules": "^3.1.0",
    "typedoc-plugin-mermaid": "^1.8.0",
    "typescript": "^4.6.4",
    "url": "^0.11.0"
  },
  "resolutions": {
    "react": "^17.*",
    "react-dom": "^17.*"
  },
  "browserslist": {
    "production": [
      ">0.95%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run lint --fix",
      "npm run format"
    ],
    "**/*.{json}": [
      "npm run format"
    ],
    "**/*.{md,mdx}": [
      "npm run format"
    ]
  }
}
